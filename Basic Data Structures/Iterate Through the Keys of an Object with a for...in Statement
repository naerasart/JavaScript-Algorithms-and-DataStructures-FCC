const users = {
  Alan: {
    online: false
  },
  Jeff: {
    online: true
  },
  Sarah: {
    online: false
  }
}

function countOnline(usersObj) {
  // Only change code below this line
   let answer=0;
for (let user in usersObj) {
  if (usersObj[user].online==true){   answer+=1 ;} else { answer+=0;}}
 return answer;  
  
  // Only change code above this line
}

console.log(countOnline(users));
