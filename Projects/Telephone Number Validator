function telephoneCheck(str) {
   
var re = /^(1\s|1)(\(\d{3}\)|\d{3})(-|\s)?\d{3}(-|\s)?\d{4}$/;
  
if(str.replace(/\D/g,'').length == 11 && re.test(str)){return true;}
var de=  /^(\(\d{3}\)|\d{3})(-|\s)?\d{3}(-|\s)?\d{4}$/;
 if(str.replace(/\D/g,'').length == 10 && de.test(str)){return true;}

return false;} 
  
  
 

telephoneCheck("555-555-5555");
