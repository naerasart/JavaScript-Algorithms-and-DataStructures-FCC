function addTogether(arg1, arg2) {
  var x;
  
  if (typeof(arg1) == "number" && typeof(arg2) == "number")
    {x= arg1 + arg2;}

 else  if (typeof(arg1) != "number" && typeof(arg2) != "number")    
   {x= undefined;}
   
  else if (arg2 === undefined)
   { x= (arg2) => addTogether(arg1,arg2);} return x;
     
}
   
