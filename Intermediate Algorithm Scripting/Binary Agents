function binaryAgent(str) {
  
var binary = str.split(" ");
var words = [];
 

for (let i = 0; i < binary.length; i++) {
  var x=parseInt(binary[i], 2); //convert to decimals
  console.log(x);
  var y=String.fromCharCode(x); // convert to letters string
  console.log(y);
   var z= words.push(y);// push to new array
    
  }
return words.join(""); //join letters to form words accordingly

}

binaryAgent("01000001 01110010 01100101 01101110 00100111 01110100 00100000 01100010 01101111 01101110 01100110 01101001 01110010 01100101 01110011 00100000 01100110 01110101 01101110 00100001 00111111");
